{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
  "rootDir": "src",
  "outDir": "dist",
  "target": "ES2022",
  "module": "CommonJS",
  "moduleResolution": "Node",
  "strict": true,
  "noImplicitAny": true,
  "noImplicitThis": true,
  "alwaysStrict": true,
  "strictNullChecks": true,
  "strictFunctionTypes": true,
  "strictBindCallApply": true,
  "strictPropertyInitialization": true,
  "esModuleInterop": true,
  "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
